{
  "name": "washingcode",
  "private": true,
  "author": {
    "name": "Artem Sapegin",
    "url": "https://sapegin.me"
  },
  "homepage": "https://github.com/sapegin/washingcode",
  "repository": "sapegin/washingcode",
  "license": "CC BY-NC-ND 4.0",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "lint": "npm run lint:js && npm run lint:text",
    "lint:js": "eslint . --cache --fix",
    "lint:text": "textlint \"manuscript/**/*.md\"",
    "lint:text:fix": "npm run lint:text -- --fix",
    "format": "prettier --loglevel warn --write \"**/*.{js,md}\"",
    "pretest": "npm run lint",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "posttest": "npm run format",
    "prepare": "husky install"
  },
  "keywords": [],
  "dependencies": {
    "immer": "^5.3.6",
    "immutable": "^4.0.0",
    "lodash": "^4.17.21",
    "react": "^18.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "eslint": "^8.14.0",
    "eslint-config-tamia": "^8.0.0",
    "glob": "^8.0.1",
    "husky": "^7.0.4",
    "jest": "^28.0.2",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "remark": "^14.0.2",
    "textlint": "^12.1.1",
    "textlint-filter-rule-comments": "^1.2.2",
    "textlint-rule-apostrophe": "^2.0.0",
    "textlint-rule-common-misspellings": "^1.0.1",
    "textlint-rule-diacritics": "^1.0.0",
    "textlint-rule-no-dead-link": "^4.8.0",
    "textlint-rule-stop-words": "^2.0.9",
    "textlint-rule-terminology": "^2.1.5",
    "textlint-rule-write-good": "^2.0.0",
    "unist-util-visit": "^4.1.0",
    "vm2": "^3.9.9"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js"
    ]
  },
  "lint-staged": {
    "*.{js,md}": "prettier --write",
    "*.js": "eslint --cache --fix"
  }
}
